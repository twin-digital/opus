{
  "$schema": "./node_modules/turbo/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": ["@twin-digital/eslint-config#build"],
      "outputs": []
    },
    "lint:fix": {
      "dependsOn": ["@twin-digital/eslint-config#build"],
      "outputs": []
    },
    "release": {
      "dependsOn": ["build"]
    },
    "test": {
      "outputs": []
    },
    "typecheck": {
      "outputs": []
    }
  }
}
