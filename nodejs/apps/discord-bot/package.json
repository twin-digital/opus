{
  "private": true,
  "name": "@twin-digital/discord-bot",
  "version": "0.1.0",
  "description": "Discord Bot providing server presence and message utilities for Twin Digital applications.",
  "type": "module",
  "files": [
    "dist",
    "!dist/**/*.d.ts.map"
  ],
  "scripts": {
    "build": "build",
    "clean": "clean",
    "deploy": "serverless deploy",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "destroy": "serverless remove",
    "dev": "pnpm exec sls-dev-tools generate | docker-dev -",
    "lint": "lint",
    "lint:fix": "lint_fix",
    "logs": "serverless logs -f hello --tail",
    "remove": "serverless remove",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@twin-digital/eslint-config": "workspace:*",
    "@twin-digital/opus-scripts": "workspace:*",
    "@twin-digital/serverless-dev-tools": "workspace:*",
    "@twin-digital/tsconfig": "workspace:*",
    "@types/aws-lambda": "^8.10.150",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "eslint-config-prettier": "catalog:",
    "globals": "catalog:",
    "prettier": "catalog:",
    "prettier-package-json": "catalog:",
    "serverless": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:"
  },
  "engines": {
    "node": "24.x",
    "pnpm": "10.x"
  }
}
